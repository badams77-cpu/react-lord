{
  "cli": {
    "version": ">= 0.38.1"
  },
  "build": {
    "development": {
      "ios": {"image": "latest",
        "applicationArchivePath": "ios/build/*.ipa"
      },
      "credentialsSource": "local",
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "developmentClient": true,
      "credentialsSource": "local",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "ios": {"image": "latest",
        "applicationArchivePath": "ios/build/*.ipa"
      },
       "credentialsSource": "local",
    }
  },
  "submit": {
    "production": {}
  }
}
